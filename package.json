{
  "name": "ai-workspace",
  "version": "1.0.0",
  "description": "AI-first workspace with session search and semantic memory",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "session:embed": "cd scripts/session-embedder && ts-node index.ts embed",
    "session:search": "cd scripts/session-embedder && ts-node search.ts",
    "session:stats": "cd scripts/session-embedder && ts-node index.ts stats",
    "tiered:search": "cd scripts/session-embedder && ts-node tiered-search.ts",
    "tiered:stats": "cd scripts/session-embedder && ts-node -e \"const { TieredMemoryManager } = require('./tiered-memory'); const m = new TieredMemoryManager(); console.log(JSON.stringify(m.getStats(), null, 2));\"",
    "tiered:maintenance": "cd scripts/session-embedder && ts-node -e \"const { TieredMemoryManager } = require('./tiered-memory'); const m = new TieredMemoryManager(); console.log(m.runMaintenance());\"",
    "hybrid:search": "cd scripts/session-embedder && ts-node hybrid-search.ts",
    "hybrid:stats": "cd scripts/session-embedder && ts-node hybrid-search.ts --stats",
    "session:score": "cd scripts/session-embedder && ts-node quality-scorer.ts",
    "list-scripts": "node scripts/list-scripts",
    "hook:session-start": "node .claude/hooks/knowledge-capture/scripts/session-start.js",
    "hook:session-end": "node .claude/hooks/knowledge-capture/scripts/session-end.js",
    "self:review": "cd scripts/self-improvement && ts-node proposal-manager.ts review",
    "self:apply": "cd scripts/self-improvement && ts-node proposal-manager.ts apply",
    "self:extract-insights": "cd scripts/self-improvement && ts-node insight-extractor.ts",
    "self:generate-reflections": "cd scripts/self-improvement && ts-node reflection-generator.ts",
    "self:propose-skills": "cd scripts/self-improvement && ts-node skill-generator.ts",
    "self:prune": "cd scripts/self-improvement && ts-node reinforcement-tracker.ts prune",
    "self:stats": "cd scripts/self-improvement && ts-node reinforcement-tracker.ts stats",
    "self:dashboard": "cd scripts/session-embedder && ts-node dashboard-generator.ts",
    "self:maintenance": "cd scripts/self-improvement && ts-node maintenance.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "session-search": "./scripts/session-embedder/search.ts"
  },
  "keywords": [
    "semantic-search",
    "session-embeddings",
    "ai-workspace",
    "claude-code",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
